{
  "extends":"eslint-config-react-app",
  "env":{
    "browser":true,
    "node":true,
    "es6":true
  },
  "parser":"babel-eslint",
  "rules":{
    "strict":0,
    "valid-jsdoc":0,
    "react/jsx-uses-react":2,
    "react/jsx-uses-vars":2,
    "react/react-in-jsx-scope":2
  },
  "plugins":[
    "react"
  ]
}